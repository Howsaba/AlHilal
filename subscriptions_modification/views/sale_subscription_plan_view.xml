<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="sale_subscription_plan_view_inherit" model="ir.ui.view">
            <field name="name">sale.subscription.plan.view.form</field>
            <field name="model">sale.subscription.plan</field>
            <field name="inherit_id" ref="sale_subscription.sale_subscription_plan_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='active']" position="after">
                    <field name="membership_in_arabic"/>
                </xpath>
                <xpath expr="//field[@name='invoice_mail_template_id']" position="after">
                    <field name="branch_id"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>