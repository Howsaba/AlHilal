<odoo>
    <data>
        <record id="view_res_team_form_inherit" model="ir.ui.view">
            <field name="name">res.team.form.inherit</field>
            <field name="model">res.team</field>
            <field name="inherit_id" ref="subscriptions_modification.res_team_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='branch_id']" position="attributes">
                    <attribute name="options">{'no_create': True}</attribute>
                </xpath>
            </field>
        </record>

